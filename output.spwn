// Automatically generated helper functions
_scgt_get = (v) => v // to counteract the clone bug in SPWN v0.8

_scgt_invert = (v) => match v.type {
    == @number: -v,
    == @bool: !v,
    in [@string, @array]: v.reverse(),
    else: v
}

// Initialize variables used
let a = null

// a.b
$.print(_scgt_get(a).b)

// !2
$.print(_scgt_invert(2))
