// Automatically generated SCGT helper functions
_scgt_explicit_print = (v) {
    $.print(v)
    return v
}

_scgt_get = (v) => v // to counteract the clone bug in spwn 0.8

_scgt_invert = (v) => match v.type {
    == @number: -v,
    == @bool: !v,
    in [@string, @array]: v.reverse(),
    else: v
}

// SCGT: gaga
$.print(_scgt_get(gaga))

// SCGT: $!ga
_scgt_explicit_print(_scgt_invert(_scgt_get(ga)))

